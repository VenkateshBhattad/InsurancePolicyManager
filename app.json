{
  "expo": {
    "name": "Insurance Policy Manager",
    "slug": "insurance-policy-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "insurancepolicymanager",
    "description": "Professional insurance policy management app for agents and brokers to manage clients, policies, and renewals efficiently.",
    "keywords": ["insurance", "policy", "management", "agent", "broker", "renewal", "client"],
    "privacy": "public",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1976d2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ankitajhawarils.insurancepolicymanager"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1976d2"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ankitajhawarils.insurancepolicymanager",
      "versionCode": 1,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 21,
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ],
      "allowBackup": false,
      "networkSecurityConfig": "./network_security_config.xml"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-notifications",
      "expo-secure-store",
      "expo-contacts",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": "1.0.0"
  }
}
